<h2>Key People</h2>

<%= form_for @game, :html => { :remote => true } do |f| %>
  <%= hidden_field_tag :element_id, @element_id %>
  <%= hidden_field_tag :helper, @helper %>
  <div class="field">
    <%= f.text_field :key_person_names, { :class => 'fcs' }, { :url => industry_people_path(), :method => :get, :param_name => 'search' } %>
  </div>
  <%= f.submit "Submit" %>
<% end %>
